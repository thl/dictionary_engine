<% assoc = spelling.category_spelling_associations.find(:all, :conditions => {:category_branch_id => 191}) %>
<% if !assoc.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header"><strong>BASIS OF SPELLING:</strong> </span>
				<%= assoc.collect{|a| link_to a.category.title, a.category.get_url_with_parent}.join(', ').html_safe %>
		</dd>
	</dl>
<% end %>
<% assoc = spelling.category_spelling_associations.find(:all, :conditions => {:category_branch_id => 638}) %>
<% if !assoc.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header"><strong>TIBETAN DIALECT:</strong> </span>
				<%= assoc.collect{|a| link_to a.category.title, a.category.get_url_with_parent}.join(', ').html_safe %>
		</dd>
	</dl>
<% end %>

<% if !spelling.analytical_note.blank? %>
<dl class="inner_dl">
	<dd class="inner_dd">
	<span class="inner_def_header"><strong>NOTE:</strong> </span>
	<span class='tinyfied_public_show'>
		<%= spelling.analytical_note.html_safe %>
	</span>
		</dd>
	</dl>
<% end %>
		
<%# if spelling.meta != nil%>
		<dl class="inner_dl">
				<dd>
		<% t = Time.now %>
		<%#= render :partial => 'public_meta', :locals => {:meta_location => t.to_f, :meta => spelling.meta , :head_id => head_id} %>
				</dd>
		</dl>
<%# end %>
