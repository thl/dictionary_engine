<% assoc = pronunciation.category_pronunciation_associations.find(:all, :conditions => {:category_branch_id => 638}) %>
<% if !assoc.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header"><strong>TIBETAN DIALECT:</strong> </span>
				<%= assoc.collect{|a| link_to a.category.title, a.category.get_url_with_parent}.join(', ').html_safe %>
		</dd>
	</dl>
<% end %>

<% if !pronunciation.analytical_note.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header">
				<strong>NOTE:</strong> 
			</span>
			<span class='tinyfied_public_show'>
				<%= pronunciation.analytical_note.html_safe %>
			</span>
		</dd>
	</dl>
<% end %>
		
<%# if !pronunciation.meta.blank? %>
		<dl class="inner_dl">
			<dd>
				<% t = Time.now %>
				<%#= render :partial => 'public_meta', :locals => {:meta_location => t.to_f, :meta => pronunciation.meta , :head_id => head_id} %>
			</dd>
		</dl>
<%# end %>
