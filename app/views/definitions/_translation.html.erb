<%# if translation.language_type != '' and translation.language_type != nil %>
<% if !translation.language_type.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header">
				<strong>LANGUAGE:</strong> 
			</span>
			<%=  link_to translation.language_type.title, translation.language_type.get_url_with_parent %>
		</dd>
	</dl>
<% end %>
<%# if translation.analytical_note != nil and translation.analytical_note != ''%>
<% if !translation.analytical_note.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header">
				<strong>NOTE:</strong> 
			</span>
			<span class='tinyfied_public_show'>
				<%= translation.analytical_note.html_safe %>
			</span>
		</dd>
	</dl>
<% end %>
		
<%# if translation.meta != nil%>
		<dl class="inner_dl">
				<dd>
		<% t = Time.now %>
		<%#= render :partial => 'public_meta', :locals => {:meta_location => t.to_f, :meta => translation.meta , :head_id => head_id} %>
				</dd>
		</dl>
<%# end %>
