<% for t in model_sentence.translations %>
	<% if !t.translation.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header">
				<strong>TRANSLATION<%= "(#{link_to t.language_type.title, t.language_type.get_url_with_parent})".html_safe if !t.language_type.blank? %>:</strong> </span>
			<span class='tinyfied_public_show'>
				<%= t.translation.html_safe %> 
			</span>
		</dd>
	</dl>
	<% end %>
	<% if !t.analytical_note.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header">
				<strong>TRANSLATION NOTE:</strong> 
			</span>
			<span class='tinyfied_public_show'>
				<%= t.analytical_note.html_safe %>
			</span>
		</dd>
	</dl>
	<% end %>
<% end %>
<% if !model_sentence.language_type.blank? %>
	<dl class="inner_dl">
		<dd class="inner_dd">
			<span class="inner_def_header">
				<strong>LANGUAGE:</strong> 
			</span><%= link_to model_sentence.language_type.title, model_sentence.language_type.get_url_with_parent if !model_sentence.language_type.blank? %>
		</dd>
	</dl>
<% end %>
<% assoc = model_sentence.category_model_sentence_associations.find(:all, :conditions => {:category_branch_id => 638}) %>
<% if !assoc.blank? %>
